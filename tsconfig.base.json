        "libs/conduit-web/feature-auth/src/index.ts"
      ],
      "@realworld-angular-nest-nx/conduit-web/ui-auth": [
        "libs/conduit-web/ui-auth/src/index.ts"
      ],
      "@realworld-angular-nest-nx/conduit-web/ui-common": [
        "libs/conduit-web/ui-common/src/index.ts"
      ],
      "@realworld-angular-nest-nx/conduit-web/utils-common": [
        "libs/conduit-web/utils-common/src/index.ts"
      ],
      "@realworld-angular-nest-nx/global": ["libs/global/src/index.ts"]
